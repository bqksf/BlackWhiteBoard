.scrollView {
    position: absolute;
    height: 100vh;
    width: 100vw;
}
.oneUserView {
    position: relative;
    height: 8vh;
    left: 50vw;
    margin-left: -45vw;
    width: 90vw;
    border-bottom: 1px solid #DBDBDB;
}

.userIconimageView {
    position: absolute;
    left: 3vw;
    height: 100rpx;
    width: 100rpx;
    top: 50%;
    margin-top: -50rpx;
    background-color: black;
    border-radius: 100rpx;
    box-shadow: 0px 0px 20px rgba(0, 0, 0, 0.1);
    border:none;
}

.userIconimage {
    position: static;
    height: 100rpx;
    width: 100rpx;
    border-radius: 100rpx;
}

.userNameView {
    position: absolute;
    left: 22vw;
    height: 100%;
    width: 100rpx;
    
}

.userNametext {
    position: absolute;
    height: 100%;
    width: 100rpx;
    top: 50%;
    margin-top: -25rpx;
}

.hover-class{
    background-color: #DBDBDB;
}